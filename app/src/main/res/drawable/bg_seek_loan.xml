<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:topRightRadius="@dimen/size4" android:bottomRightRadius="@dimen/size4"/>
            <solid android:color="#E6E6E6"/>

        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:topLeftRadius="@dimen/size4" android:bottomLeftRadius="@dimen/size4"/>
                <gradient
                    android:startColor="#FFE807"
                    android:endColor="#FFCC03"
                    android:centerY="0.5"
                    android:angle="270"
                    />
            </shape>
        </clip>
    </item>
</layer-list>